<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="oidc:RedirectUri" value="http://localhost/phe_actris_bypasslogin/"   xdt:Transform="SetAttributes" xdt:Locator="Match(key)"  />
	</appSettings>
	<system.web>
		<compilation xdt:Transform="RemoveAttributes(debug)" />
		<authentication mode="Forms" xdt:Transform="SetAttributes">
			<forms defaultUrl="~/" loginUrl="~/Account/Login" slidingExpiration="true" timeout="2880" xdt:Transform="SetAttributes"></forms>
		</authentication>
	</system.web>
</configuration>