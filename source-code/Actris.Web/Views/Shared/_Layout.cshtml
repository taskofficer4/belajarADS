<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - Application Standard Development</title>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">

    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/fontawesome/css/bundle")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    <!--This bundle was moved by the Telerik VS Extensions for compatibility reasons-->
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
  
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
</head>
<body>
    <div>
        @{Html.RenderAction("UserHeader", "Account");}
        <div>

            @Html.Partial("Layout/_Sidebar")
            <main>
                @RenderBody()
            </main>
        </div>
    </div>


    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="~/Scripts/moment/moment.js"></script>
    <script src="~/Scripts/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/autonumeric/autonumeric.min.js"></script>
    <script src="~/Scripts/select2/select2.js"></script>
    <script src="~/Scripts/SingleSidebar.js"></script>
    <script src="~/Scripts/summernote/summernote-bs5.min.js"></script>
</body>


</html>